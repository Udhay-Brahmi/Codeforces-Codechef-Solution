#include <bits/stdc++.h>
using namespace std;
int main(){;
    int n,a,b,m=0,c=0;cin>>n;
    while(n--){
        cin>>a>>b;
        if(a>b){m++;}
        else if(a<b){c++;}
    }
    if(c==m){
        cout<<"Friendship is magic!^^" <<endl;
    }
    else if(c>m){
        cout<<"Chris" <<endl;
    }
    else{
        cout<<"Mishka"<<endl;
    }
    return 0;
}
