#include <bits/stdc++.h>
using namespace std;
int main(){
  int n,m;cin>>n>>m;
  int y =(7 - max(n,m));
  if(y==1){cout<<1<<"/"<<6<<endl;}
  if(y==2){cout<<1<<"/"<<3<<endl;}
  if(y==3){cout<<1<<"/"<<2<<endl;}
  if(y==4){cout<<2<<"/"<<3<<endl;}
  if(y==5){cout<<5<<"/"<<6<<endl;}
  if(y==6){cout<<1<<"/"<<1<<endl;}
  if(y==0){cout<<0<<"/"<<1<<endl;}
}
