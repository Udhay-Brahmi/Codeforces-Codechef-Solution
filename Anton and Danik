#include <bits/stdc++.h>
using namespace std;
int main(){
    int n,a=0,b=0;string h;
    cin>>n;
    cin>>h;
    for(int i=0;i<n;i++){
               if(h[i]=='A'){a++;}
               else{b++;}
    }
    if(a>b){cout<<"Anton"<<endl;}
    else if (a<b){cout<<"Danik"<<endl;}
    else{cout<<"Friendship"<<endl;}
    return 0;
}
