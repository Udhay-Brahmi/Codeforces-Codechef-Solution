#include <bits/stdc++.h>
using namespace std;
int main(){
    int n,x=0;cin>>n;
    while(n--){
        string h;
        cin>>h;
        for(int i=0;i<h.length();i++){
            if(h[i]=='+'){x++;break;}
            else if(h[i]=='-'){x--;break;}
        }
    }
    cout<<x<<endl;
    return 0;
}
